<% if @picture.new_record? %>
  alert("Failed to upload painting: <%= j @picture.errors.full_messages.join(', ').html_safe %>");
<% else %>
  $("div#pictures").append("<%= j render(:partial=>"picture_view.html") %>");

  $($("div#<%=@picture.id%>.file-list-item div.file-item")).find("#logo-links").bind("click",
  function(){
  var href = $($(this)[0]).find('a').attr('href');
  window.location.href = href
  console.log($($(this)[0]).find('a').attr('href'));
  });
<% end %>